# 3dPot Platform - Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Application
VITE_APP_NAME=3dPot Platform
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Plataforma de Prototipagem 3D com IA

# Development
VITE_NODE_ENV=development
VITE_DEBUG=true

# Features
VITE_ENABLE_CHAT=true
VITE_ENABLE_HISTORY=true
VITE_ENABLE_DASHBOARD=true

# API Timeouts (ms)
VITE_API_TIMEOUT=10000
VITE_WS_TIMEOUT=30000

# Retries
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000